Configs: {
	OrchidClientConfig: {
		DisplayName: Клиентская конфигурация

		Headers: {
			General: Основной раздел
			Guardian: Хранитель
			Shapeshifter: Оборотень
		}

		ShowClassTags: {
			Label: Показывать метки классов
